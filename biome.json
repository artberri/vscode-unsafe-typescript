{
	"files": {
		"ignore": ["**/out/**"]
	},
	"formatter": {
		"enabled": true,
		"lineWidth": 80,
		"indentStyle": "tab",
		"lineEnding": "lf"
	},
	"linter": {
		"enabled": true,
		"ignore": ["example/**"],
		"rules": {
			"recommended": true,
			"suspicious": {
				"noConsoleLog": "error"
			}
		}
	},
	"organizeImports": {
		"enabled": true
	}
}
